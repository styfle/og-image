{
    "name": "og-image",	 
    "alias": [ "og-image.now.sh", "api-og-image.zeit.sh" ],
    "version": 2,
    "regions": ["all"],
    "public": true,
    "routes": [
        { "src": "/", "dest": "/index.html" },
        { "src": "/(favicon.ico|style.css|robots.txt|browser.js)", "dest": "$1" },
        { "src": "/(.+)", "dest": "/api" }
    ]
}
